class Alumno:
    def __init__(self, nombre, nota):
        self.nombre=nombre
        self.nota=nota

    def __str__(self):
        return 'Nombre {}, Nota {}'.format(self.nombre,self.nota)


alumno1=Alumno('Renny',6)
alumno2=Alumno('Jose',5)
alumno3=Alumno('Maria',7)

print(alumno1)
print(alumno2)
print(alumno3)
